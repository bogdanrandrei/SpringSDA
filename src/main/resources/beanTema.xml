<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="EmailPrinter" class="TemaSpring.EmailPrinter">
        <constructor-arg value="Email printer">
        </constructor-arg>
    </bean>

    <bean id="PaperPrinter" class="TemaSpring.PaperPrinter">
        <constructor-arg value="PaperPrinter">
        </constructor-arg>
    </bean>

    <bean id="PapyrusPrinter" class="TemaSpring.PapyrusPrinter">
        <constructor-arg value="PapyrusPrinter">
        </constructor-arg>
    </bean>
</beans>